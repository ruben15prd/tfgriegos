<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ruben.connecttomysql">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListFarmsActivity" />
        <activity android:name=".DisplayFarmActivity" />
        <activity android:name=".EditFarmActivity" />
        <activity android:name=".CreateFarmActivity" />
        <activity android:name=".ListPlotsActivity" />
        <activity android:name=".DisplayPlotActivity" />
        <activity android:name=".EditPlotActivity" />
        <activity android:name=".CreatePlotActivity" />
        <activity android:name=".DisplayClimatologicalProbeActivity" />
        <activity android:name=".EditClimatologicalProbeActivity"></activity>
    </application>

</manifest>